@model IEnumerable<Exam.Models.Tweet>


@if (Model != null)
{
    foreach (var tweet in Model)
    {
        <div>
            <h3>Text</h3>
            <div>@tweet.Text</div>
        </div>
        <div>
            <h3>DatePosted</h3>
            <div>@tweet.DatePosted</div>
        </div>
    }

    if (Model.Count() == 0)
    {
        <div>No results found!</div>
    }
}
